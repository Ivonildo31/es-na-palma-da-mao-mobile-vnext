        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.716051840979-no1a2uaro7tun9f7timu90c7rpoe7heg" />
    </plugin>
    <plugin name="cordova-sqlite-storage" spec="^2.2.1" />
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
</widget>
